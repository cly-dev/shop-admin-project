    "core-js": "^3.8.3",
    "dotenv": "^16.0.3",
    "element-plus": "^2.2.17",
    "husky": "^8.0.3",
    "jsencrypt": "^3.2.1",
    "lint-staged": "^13.2.0",
    "pinia": "^2.0.23",
    "pinia-plugin-persist": "^1.0.0",
    "register-service-worker": "^1.7.2",
    "vue": "^3.2.13",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0",
    "wangeditor": "^4.6.3"
